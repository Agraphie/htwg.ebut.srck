<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="de.htwg_konstanz.ebus.framework.wholesaler.vo">
	<class name="PaymentInvoicePurchase" table="payment_invoice_purchase">

		<composite-id>
			<key-property name="Partialpaymentnumber" column="partialpaymentnumber"
				type="integer" />
			<key-property name="Payment" column="payment"
				type="string" />
			<key-property name="Invoice" column="invoice"
				type="string" />
			<key-property name="Invoiceitem" column="invoiceitem"
				type="integer" />
		</composite-id>

	</class>
</hibernate-mapping>
